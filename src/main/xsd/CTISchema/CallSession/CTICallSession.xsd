<xs:schema xmlns="http://schema.broadsoft.com/CTI"
  targetNamespace="http://schema.broadsoft.com/CTI" xmlns:xs="http://www.w3.org/2001/XMLSchema"
  xmlns:xsp="http://schema.broadsoft.com/XspXMLInterface" xmlns:xsi="http://schema.broadsoft.com/xsi"
  elementFormDefault="unqualified" version="1.0">

  <xs:import namespace="http://schema.broadsoft.com/XspXMLInterface"
    schemaLocation="../../XSPXMLSchema/XspXMLInterface.xsd" />

  <xs:import namespace="http://schema.broadsoft.com/xsi"
    schemaLocation="../../XSISchema/CallSession/XSICallSession.xsd" />

    <xs:element name="AllocateIMRNRequest" substitutionGroup="xsp:payload">
    <xs:annotation>
      <xs:documentation>
        This request is used by a remote application to allocate an IMRN 
        resource on BroadWorks to anchor a mobile call.
        
        Returns a generic xsp:Response with an xsi:IMRNInfo. 
        
        Change History:
        19.0 - Added.   
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:complexContent>
        <xs:restriction base="xsp:RequestPayload">
          <xs:sequence>
            <xs:element name="uri" type="xs:anyURI">
              <xs:annotation>
                <xs:documentation>
                  The relative URI of the XSI Blind Transfer request
                  I.e. "/-context-/v2.0/user/%subscriberid%/calls/imrn?%callingPartyAddress%&amp;%calledPartyAddress%""
                  where -context- corresponds to the application context of the Xsi-Actions web application (e.g. com.broadsoft.xsi-actions).  
                </xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="method" type="xs:token" fixed="PUT" />
            <xs:element name="version" type="xs:token" />            
            <xs:element name="params">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="subscriberid" type="xsi:SubscriberId">
                    <xs:annotation>
                      <xs:documentation>
                        The subscriberId associated to
                        the call to transfer.
                      </xs:documentation>
                    </xs:annotation>
                  </xs:element>                  
                  <xs:element name="callingPartyAddress" type="xsi:NonEmptyToken">
                    <xs:annotation>
                      <xs:documentation>
                        The destination address for the
                        transfer.
                    </xs:documentation>
                    </xs:annotation>
                  </xs:element>
                   <xs:element name="calledPartyAddress" type="xsi:NonEmptyToken">
                    <xs:annotation>
                      <xs:documentation>
                        The destination address for the
                        transfer.
                    </xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element name="payload" type="xsp:EmptyPayload" />
          </xs:sequence>
        </xs:restriction>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>

  <xs:element name="BlindTransferRequest" substitutionGroup="xsp:payload">
    <xs:annotation>
      <xs:documentation>
        This request is used to transfer an existing
        call to a new destination. This transfer is performed 
        in a single-step, that is the client application 
        doing the transfer does not have to place the existing call 
        on hold before issuing the transfer. The subscriber must 
        have the Call Transfer service in order to use this request.
        
        A call can be transferred when it is in the Alerting, 
        Held, Remote Held and Active state. When it is in the 
        Alerting state, it must be have its personality set to
        Terminator.

        This returns a generic xsp:Response with empty
        Payload    
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:complexContent>
        <xs:restriction base="xsp:RequestPayload">
          <xs:sequence>
            <xs:element name="uri" type="xs:anyURI">
              <xs:annotation>
                <xs:documentation>
                  The relative URI of the XSI Blind Transfer request
                  I.e. "/-context-/v2.0/user/%subscriberid%/calls/%callid%/blindtransfer?%address%"
                  where -context- corresponds to the application context of the Xsi-Actions web application (e.g. com.broadsoft.xsi-actions).  
                </xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="method" type="xs:token" fixed="PUT" />
            <xs:element name="version" type="xs:token" />            
            <xs:element name="params">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="subscriberid" type="xsi:SubscriberId">
                    <xs:annotation>
                      <xs:documentation>
                        The subscriberId associated to
                        the call to transfer.
                      </xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="callid" type="xs:string">
                    <xs:annotation>
                      <xs:documentation>
                        The callId of the call to
                        transfer.
                    </xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="address" type="xsi:Address">
                    <xs:annotation>
                      <xs:documentation>
                        The destination address for the
                        transfer.
                    </xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element name="payload" type="xsp:EmptyPayload" />
          </xs:sequence>
        </xs:restriction>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>

  <xs:element name="CallMeNowRequest" substitutionGroup="xsp:payload">
    <xs:annotation>
      <xs:documentation>
        This request is used to establish a call session between 
        an external party and the specified subscriber. BroadWorks
        initially alerts the external party using the specified address, 
        and upon answer, alerts the specified subscriber.

        BroadWorks performs basic validation of the request and, 
        if it is accepted, returns a CallMeNowStartInfo element.
        If the specified subscriber has the Call Me Now service's 
        Answer Confirmation option set to "Passcode", then the 
        CallMeNowStartInfo returned in the success response 
        includes the passcode to be used by the external party.
        
        This request does not require authentication credentials.

        Returns a xsp:Response with xsi:CallMeNowStartInfo.
        
        Change History:
        17.sp4 - Added.
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:complexContent>
        <xs:restriction base="xsp:RequestPayload">
          <xs:sequence>
            <xs:element name="uri" type="xs:anyURI">
              <xs:annotation>
                <xs:documentation>
                  The relative URI of the XSI Call Me Now request
                  I.e. "/-context-/v2.0/user/%subscriberid%/calls/CallMeNow?%address%&amp;%transactionId%"
                  where -context- corresponds to the application context of the Xsi-Actions web application (e.g. com.broadsoft.xsi-actions).  
                </xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="method" type="xs:token" fixed="POST" />
            <xs:element name="version" type="xs:token"></xs:element>            
            <xs:element name="params">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="subscriberid" type="xsi:SubscriberId">
                    <xs:annotation>
                      <xs:documentation>
                        The subscriberId of the BroadWorks user receiving the 
                        call.
                    </xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="address" type="xsi:Address">
                    <xs:annotation>
                      <xs:documentation>
                        Address of the external party to alert.
                    </xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="transactionId" type="xsi:NonEmptyString" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>
                        When present, the transactionId parameter is captured in the 
                        originating CDR created for the Call-Me-Now call from the target 
                        user to the external party.  It can be used for various purposes 
                        such as capturing an identifier of the application or external 
                        party that is invoking the Call Me Now service.  
                        It is an optional parameter, and any string value up to 128 
                        characters long is valid.
                    </xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element name="payload" type="xsp:EmptyPayload" />
          </xs:sequence>
        </xs:restriction>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>

  <xs:element name="CallReturnRequest" substitutionGroup="xsp:payload">
    <xs:annotation>
      <xs:documentation>
        The Call Return request is used to initiate a
        call using the address of the last call missed 
        or received by the subscriber. The user must have 
        the Call Return service in order to use this request.

        When the request is received, BroadWorks performs basic
        validation and, if the request is accepted, it
        initiates a call towards the subscriber, assigns 
        a callId and an externalTrackingId to the connection 
        and returns it in the response.
        
        Like for the Dial request, BroadWorks alerts the
        subscriber's endpoint prompting the subscriber to 
        go off-hook. In some cases, the endpoint may support 
        protocol extensions to immediately go off-hook without 
        requiring a subscriber action.

        Returns a xsp:Response with xsi:CallStartInfo payload.
        
        Change History:
        17.sp4 - Modified to add the location and locationAddress 
                 optional elements.
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:complexContent>
        <xs:restriction base="xsp:RequestPayload">
          <xs:sequence>
            <xs:element name="uri" type="xs:anyURI">
              <xs:annotation>
                <xs:documentation>
                  The relative URI of the XSI CallReturn request
                  I.e. "/-context-/v2.0/user/%subscriberid%/calls/callreturn?%location%&amp;%locationAddress%"
                  where -context- corresponds to the application context of the Xsi-Actions web application (e.g. com.broadsoft.xsi-actions).  
                </xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="method" type="xs:token" fixed="POST" />
            <xs:element name="version" type="xs:token"></xs:element>            
            <xs:element name="params">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="subscriberid" type="xsi:SubscriberId">
                    <xs:annotation>
                      <xs:documentation>
                        The subscriber that returns the
                        call.
                    </xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="location" type="xsi:ClickToDialLocationEnum" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>
                        When present, specifies the location of the 
                        calling user to ring for the call.
                        
                        When not present, locations are alerted per the 
                        calling user's profile on BroadWorks.
                    </xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="locationAddress" type="xsi:NonEmptyToken" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>
                        Specific address of location to ring. This element 
                        should only be present if the "location" element is 
                        present and set to "BroadWorksAnywhere" or 
                        "SharedCallAppearance".
                                          
                        When the "location" element is set to 
                        "BroadWorksAnywhere", the locationAddress is 
                        optional and contains a directory number. The 
                        directory number may be in E.164 format or it 
                        may just contain digits. When not present, all
                        BroadWorks Anywhere locations are alerted. The 
                        following are valid directory numbers for the 
                        locationAddress element:
                              - 9725551000
                              - +19725551000

                        When the "location" element is set to 
                        "SharedCallAppearance", the locationAddress 
                        is optional and contains a BroadWorks 
                        address of record.  The address of record may 
                        be user@domain for SIP locations or user for 
                        MGCP locations. When not present, all
                        Shared Call Appearance locations are alerted.
                        The following are valid address of records for 
                        the locationAddress element:
                              - 9725551000@broadsoft.com
                              - 9725551000
                      </xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element name="payload" type="xsp:EmptyPayload" />
          </xs:sequence>
        </xs:restriction>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>

  <xs:element name="ConsultativeTransferRequest"
    substitutionGroup="xsp:payload">
    <xs:annotation>
      <xs:documentation>
        This request is used to transfer a specified
        call after consulting with the receiving party. The Consultative
        Transfer is also known as an Attended Transfer, or a Transfer
        with Consultation. The subscriber must have the Call Transfer
        service in order to use this request. 
        
        From a call topology point of view, the Consultative 
        Transfer merges two call sessions into a new one. 
        All calls are maintained but are connected to the new
        call session.

        Returns generic xsp:Response with empty payload.
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:complexContent>
        <xs:restriction base="xsp:RequestPayload">
          <xs:sequence>
            <xs:element name="uri" type="xs:anyURI">
              <xs:annotation>
                <xs:documentation>
                  The relative URI of the XSI Consulative Transfer request
                  I.e. "/-context-/v2.0/user/%subscriberid%/calls/%callid1%/ConsultTransfer/%callid2%"
                  where -context- corresponds to the application context of the Xsi-Actions web application (e.g. com.broadsoft.xsi-actions).  
                </xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="method" type="xs:token" fixed="PUT" />
            <xs:element name="version" type="xs:token"></xs:element>            
            <xs:element name="params">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="subscriberid" type="xsi:SubscriberId">
                    <xs:annotation>
                      <xs:documentation>
                        The subscriberId associated to
                        the call to transfer.
                      </xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="callid1" type="xsi:CallId">
                    <xs:annotation>
                      <xs:documentation>
                        The callId of the call to
                        transfer.
                    </xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="callid2" type="xsi:CallId">
                    <xs:annotation>
                      <xs:documentation>
                        The callId of the consulted call.
                    </xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element name="payload" type="xsp:EmptyPayload" />
          </xs:sequence>
        </xs:restriction>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>

  <xs:element name="CustomerOriginatedTraceRequest"
    substitutionGroup="xsp:payload">
    <xs:annotation>
      <xs:documentation>
        This request is used to trace a call. The trace
        is done by generating an SNMP alarm with the caller ID
        information. The subscriber must have the Customer 
        Originated Trace (COT) service assigned in order to 
        use this request successfully.
        
        If the request specifies a callId, then the trace
        is made for the corresponding call. This can only be 
        done if the call is in the Active, Held or Remote 
        Held state with Terminator personality and can only 
        be performed once per call. 
        
        If the request does not specify a callId, then the 
        trace is made for the subscriber's most recently 
        received/missed call. Multiple invocation of COT 
        on the same call will result in only one single 
        trace being generated. The execution of this request 
        does not create session or a call.

        Returns generic xsp:Response with
        Empty payload.
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:complexContent>
        <xs:restriction base="xsp:RequestPayload">
          <xs:sequence>
            <xs:element name="uri" type="xs:anyURI">
              <xs:annotation>
                <xs:documentation>
                  The relative URI of the XSI Customer Originated Trace request
                  I.e. "/-context-/v2.0/user/%subscriberid%/calls/CustomerOriginatedTrace?%callId%"
                  where -context- corresponds to the application context of the Xsi-Actions web application (e.g. com.broadsoft.xsi-actions).  
                </xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="method" type="xs:token" fixed="POST" />
            <xs:element name="version" type="xs:token"></xs:element>            
            <xs:element name="params">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="subscriberid" type="xsi:SubscriberId">
                    <xs:annotation>
                      <xs:documentation>
                        The subscriberId associated to
                        the customer originated trace.
                      </xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="callId" type="xsi:CallId"
                    minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>
                        The callId for the call for
                        which the Customer Originated Trace
                        is requested.
                        If not present, then the trace is
                        made for the subscriber's most recently
                        received/missed call.
                      </xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element name="payload" type="xsp:EmptyPayload" />
          </xs:sequence>
        </xs:restriction>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>

  <xs:element name="DialRequest" substitutionGroup="xsp:payload">
    <xs:annotation>
      <xs:documentation>
        This request is used to originate a call session
        from the specified subscriber to the specified destination.
        BroadWorks performs basic validation of the request and, 
        if it is accepted, then it assigns a callId and an 
        externalTrackingId to the connection and returns it in 
        the response.

        Returns a xsp:Response with xsi:CallStartInfo.
        
        Change History:
        17.sp4 - Modified to add the location and locationAddress 
                 optional elements.
        20.0   - Added parameters: info, conferenceId, pin
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:complexContent>
        <xs:restriction base="xsp:RequestPayload">
          <xs:sequence>
            <xs:element name="uri" type="xs:anyURI">
              <xs:annotation>
                <xs:documentation>
                  The relative URI of the XSI Dial request
                  I.e. "/-context-/v2.0/user/%subscriberid%/calls/new?%address%&amp;%clid%&amp;%location%&amp;%locationAddress%&amp;%info%&amp;%conferenceId%&amp;%pin%"
                  where -context- corresponds to the application context of the Xsi-Actions web application (e.g. com.broadsoft.xsi-actions).  
                </xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="method" type="xs:token" fixed="POST" />
            <xs:element name="version" type="xs:token"></xs:element>            
            <xs:element name="params">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="subscriberid" type="xsi:SubscriberId">
                    <xs:annotation>
                      <xs:documentation>
                        The subscriberId of the calling
                        user.
                    </xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="address" type="xsi:Address">
                    <xs:annotation>
                      <xs:documentation>
                        Address of destination Phone no.
                        to call.
                    </xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="clid" type="xsi:NonEmptyToken" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>
                        The element is optional and is used for calls placed 
                        by call center agents to specify the calling line 
                        identity (CLID) to be presented.  
                        
                        This is used when a call center agent must place a 
                        call with an automatic call distribution (ACD) number 
                        different from their default configuration. It is also
                        used when a call center agent places a call with their 
                        personal calling line ID.
                        
                        The value "personal" is specified if the call 
                        center agent places a personal call. Otherwise, the value
                        must match the address of an automatic call 
                        distribution (ACD) to which the call center agent is 
                        assigned.
                      </xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="location" type="xsi:ClickToDialLocationEnum" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>
                        When present, specifies the location of the 
                        calling user to ring for the call.
                        
                        When not present, locations are alerted per the 
                        calling user's profile on BroadWorks.
                    </xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="locationAddress" type="xsi:NonEmptyToken" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>
                        Specific address of location to ring. This element 
                        should only be present if the "location" element is 
                        present and set to "BroadWorksAnywhere" or 
                        "SharedCallAppearance".
                                          
                        When the "location" element is set to 
                        "BroadWorksAnywhere", the locationAddress is 
                        optional and contains a directory number. The 
                        directory number may be in E.164 format or it 
                        may just contain digits. When not present, all
                        BroadWorks Anywhere locations are alerted. The 
                        following are valid directory numbers for the 
                        locationAddress element:
                              - 9725551000
                              - +19725551000

                        When the "location" element is set to 
                        "SharedCallAppearance", the locationAddress 
                        is optional and contains a BroadWorks 
                        address of record.  The address of record may 
                        be user@domain for SIP locations or user for 
                        MGCP locations. When not present, all
                        Shared Call Appearance locations are alerted.
                        The following are valid address of records for 
                        the locationAddress element:
                              - 9725551000@broadsoft.com
                              - 9725551000
                      </xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="info" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>
                       This is an opaque information provided by the client for the 
                       Application Server to correlate this information with the call 
                       session. The Application Server stores this information against 
                       the call session and triggers notifications for both ends of the 
                       call session over the SIP or CTI interface.   
                    </xs:documentation>
                    </xs:annotation>
                  </xs:element>         
                  <xs:element name="conferenceId" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>
                       This is the conference ID for the conference to join.   
                    </xs:documentation>
                    </xs:annotation>
                  </xs:element>             
                  <xs:element name="pin" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>
                       This is the security PIN for the conference to join, if the 
                       conference requires a security PIN.
                    </xs:documentation>
                    </xs:annotation>
                  </xs:element>       
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element name="payload" type="xsp:EmptyPayload" />
          </xs:sequence>
        </xs:restriction>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>

  <xs:element name="DirectedCallPickupRequest"
    substitutionGroup="xsp:payload">
    <xs:annotation>
      <xs:documentation>
        The Directed Call Pickup request is used to
        pickup (answer) a call directed to another 
        subscriber in the same customer group (or enterprise, 
        if the group is part of an enterprise).

        Returns a xsp:Response with xsi:CallStartInfo.
        
        Change History:
        17.sp4 - Modified to add the location and locationAddress 
                 optional elements.
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:complexContent>
        <xs:restriction base="xsp:RequestPayload">
          <xs:sequence>
            <xs:element name="uri" type="xs:anyURI">
              <xs:annotation>
                <xs:documentation>
                  The relative URI of the XSI Directed Call Pickup
                  I.e. "/-context-/v2.0/user/%subscriberid%/calls/DirectedCallPickup?%address%&amp;%location%&amp;%locationAddress%"
                  where -context- corresponds to the application context of the Xsi-Actions web application (e.g. com.broadsoft.xsi-actions).  
                </xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="method" type="xs:token" fixed="POST" />
            <xs:element name="version" type="xs:token"></xs:element>            
            <xs:element name="params">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="subscriberid" type="xsi:SubscriberId">
                    <xs:annotation>
                      <xs:documentation>
                        The subscriberId performing the
                        call pickup.
                    </xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="address" type="xsi:Address">
                    <xs:annotation>
                      <xs:documentation>
                        The address of the user to pickup against.
                    </xs:documentation>
                    </xs:annotation>
                  </xs:element>                  
                  <xs:element name="location" type="xsi:ClickToDialLocationEnum" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>
                        When present, specifies the location of the 
                        calling user to ring for the call.
                        
                        When not present, locations are alerted per the 
                        calling user's profile on BroadWorks.
                    </xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="locationAddress" type="xsi:NonEmptyToken" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>
                        Specific address of location to ring. This element 
                        should only be present if the "location" element is 
                        present and set to "BroadWorksAnywhere" or 
                        "SharedCallAppearance".
                                          
                        When the "location" element is set to 
                        "BroadWorksAnywhere", the locationAddress is 
                        optional and contains a directory number. The 
                        directory number may be in E.164 format or it 
                        may just contain digits. When not present, all
                        BroadWorks Anywhere locations are alerted. The 
                        following are valid directory numbers for the 
                        locationAddress element:
                              - 9725551000
                              - +19725551000

                        When the "location" element is set to 
                        "SharedCallAppearance", the locationAddress 
                        is optional and contains a BroadWorks 
                        address of record.  The address of record may 
                        be user@domain for SIP locations or user for 
                        MGCP locations. When not present, all
                        Shared Call Appearance locations are alerted.
                        The following are valid address of records for 
                        the locationAddress element:
                              - 9725551000@broadsoft.com
                              - 9725551000
                      </xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
