  <xs:element name="GetMeetMeConferenceRequest" substitutionGroup="xsp:payload">
    <xs:annotation>
      <xs:documentation>
        This request is used to get details of a Meet-Me conference. 

        Returns a xsp:Response with xsi:MeetMeConference.
        
        Change History:
        19.0 - Added.   
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:complexContent>
        <xs:restriction base="xsp:RequestPayload">
          <xs:sequence>
            <xs:element name="uri" type="xs:anyURI">
              <xs:annotation>
                <xs:documentation>
                  The relative URI of the request
                  I.e. "/-context-/v2.0/meetmeconference/%bridgeId%/conference/%conferenceId%"
                  where -context- corresponds to the application context of the Xsi-Actions web application (e.g. com.broadsoft.xsi-actions).  
                </xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="method" type="xs:token" fixed="GET" />
            <xs:element name="version" type="xs:token" />            
            <xs:element name="params">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="bridgeId" type="xsi:SubscriberId">
                    <xs:annotation>
                      <xs:documentation>
                        The subscriberId of the Meet-Me bridge.
                    </xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="conferenceId" type="xsi:MeetMeConferenceId">
                    <xs:annotation>
                      <xs:documentation>
                        The conferenceId of the conference.
                    </xs:documentation>
                    </xs:annotation>
                  </xs:element>                  
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element name="payload" type="xsp:EmptyPayload" />
          </xs:sequence>
        </xs:restriction>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>

